{
  "name": "polina-grow",
  "version": "1.0.0",
  "description": "The best application",
  "main": "index.js",
  "engines": {
    "npm": ">=5.6.0",
    "node": ">=8.11.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint **/*.js, **/*.jsx --ignore-pattern node_modules/",
    "lint:fix": "eslint --ignore-path .gitignore --ignore-pattern node_modules/ --fix",
    "prettify": "prettier --write",
    "dev": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavel06081991/polina-grow.git"
  },
  "author": "Polina Kartel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pavel06081991/polina-grow/issues"
  },
  "homepage": "https://github.com/pavel06081991/polina-grow#readme",
  "babel": {
    "presets": ["env", "react"],
    "plugins": [ "react-hot-loader/babel" ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.1",
    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.10",
    "lint-staged": "^7.2.0",
    "prettier": "1.13.7",
    "prettier-eslint": "^8.8.2",
    "react-hot-loader": "^4.3.4",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "immutable": "^3.8.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "styled-components": "^3.3.3"
